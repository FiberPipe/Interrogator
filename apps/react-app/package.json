{
  "name": "@interrogator/app",
  "private": true,
  "version": "1.0.0",
  "main": "build/src/app/electron/main.js",
  "scripts": {
    "dev:react": "rsbuild dev --port 3000",
    "dev:web": "npm run dev:react -- --open",
    "dev:electron": "concurrently \"pnpm dev:react\" \"wait-on http://127.0.0.1:3000 && tsc -p ./src/app/electron/ -w\" \"wait-on http://127.0.0.1:3000 && tsc -p ./src/app/electron/ && electron .\"",
    "dev": "npm run dev:web",
    "build:web": "rsbuild build",
    "build:electron": "npm run build:web && tsc -p ./src/app/electron/ && electron-builder",
    "build:win": "npm run build:web && tsc -p ./src/app/electron/ && electron-builder --win --x64"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@heroui/react": "2.7.5",
    "@nextui-org/react": "^2.4.6",
    "bem-cn-lite": "4.1.0",
    "echarts": "5.6.0",
    "electron-store": "10.1.0",
    "knex": "3.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.25.1",
    "recharts": "^2.12.7",
    "serialport": "13.0.0",
    "zustand": "4.5.4"
  },
  "devDependencies": {
    "@gravity-ui/icons": "2.14.0",
    "@rsbuild/core": "1.0.1-beta.5",
    "@rsbuild/plugin-react": "1.0.1-beta.5",
    "@types/node": "22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "8.2.2",
    "electron": "31.3.0",
    "electron-builder": "26.0.12",
    "electronmon": "2.0.3",
    "framer-motion": "^11.3.19",
    "postcss": "^8.4.40",
    "postcss-loader": "^8.1.1",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.2",
    "wait-on": "7.2.0"
  },
  "build": {
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}