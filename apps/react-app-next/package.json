{
  "name": "react-app-next",
  "version": "1.0.0",
  "private": true,
  "main": "build/src/app/electron/main.js",
  "scripts": {
    "build": "rsbuild build",
    "preview": "rsbuild preview",
    "dev:react": "rsbuild dev --port 3000",
    "dev:web": "npm run dev:react -- --open",
    "dev:electron": "concurrently \"pnpm dev:react\" \"wait-on http://127.0.0.1:3000 && tsc -p ./src/app/electron/ -w\" \"wait-on http://127.0.0.1:3000 && tsc -p ./src/app/electron/ && electron .\"",
    "dev:electron:proto": "pnpm postbuild && pnpm dev:electron",
    "prod:electron": "cross-env PRODUCTION=true && pnpm postBuild && pnpm dev:electron",
    "dev": "npm run dev:web",
    "postbuild": "cp -r ./src/app/electron/api/proto ./build/src/app/electron/api/",
    "build:web": "rsbuild build",
    "build:electron": "pnpm build:web && pnpm postBuild && tsc -p ./src/app/electron/ && electron-builder",
    "build:prod:electron": "cross-env PRODUCTION=true npm run build:web && tsc -p ./src/app/electron/ && electron-builder",
    "download:tiles": "npx ts-node ./src/shared/utils/download-tiles.ts"
  },
  "dependencies": {
    "@gravity-ui/uikit": "7.18.0",
    "@grpc/grpc-js": "1.13.4",
    "@grpc/proto-loader": "0.8.0",
    "@nivo/core": "0.99.0",
    "@nivo/line": "0.99.0",
    "@rsbuild/plugin-sass": "1.3.3",
    "@types/node": "22.1.0",
    "electron": "31.3.0",
    "electron-log": "5.4.3",
    "leaflet": "1.9.4",
    "node-fetch": "3.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^2.12.7"
  },
  "devDependencies": {
    "@gravity-ui/graph": "1.1.4",
    "@gravity-ui/icons": "2.14.0",
    "@gravity-ui/navigation": "3.6.3",
    "@rsbuild/core": "^1.4.3",
    "@rsbuild/plugin-react": "^1.3.3",
    "@rsbuild/plugin-svgr": "1.2.1",
    "@types/leaflet": "1.9.20",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "bem-cn-lite": "4.1.0",
    "concurrently": "8.2.2",
    "cross-env": "10.0.0",
    "electron-icon-maker": "0.0.5",
    "i18next": "25.4.0",
    "i18next-browser-languagedetector": "8.2.0",
    "react-i18next": "15.7.1",
    "react-router-dom": "^6.25.1",
    "typescript": "^5.8.3",
    "wait-on": "7.2.0",
    "zustand": "4.5.4"
  }
}
